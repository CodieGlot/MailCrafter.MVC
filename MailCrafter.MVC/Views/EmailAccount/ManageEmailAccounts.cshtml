@model List<string>

@{
    ViewData["Title"] = "Manage Email Accounts";
}

<div class="manage-email-accounts-container">
    <h1>Manage Email Accounts</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var email in Model)
            {
                <tr>
                    <td>@email</td>
                </tr>
            }
        </tbody>
    </table>

    <a asp-action="AddEmailAccount" class="btn btn-primary">Add Email Account</a>
</div>

